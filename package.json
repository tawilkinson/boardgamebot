		"@typescript-eslint/eslint-plugin": "^4.29.3",
		"@typescript-eslint/parser": "^4.29.3",
		"all-contributors-cli": "^6.19.0",
		"eslint": "^7.32.0",
		"jest": "^27.0.6",
		"jest-circus": "^27.0.6",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.3.2",
