    "@types/node-fetch": "^2.5.4",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "all-contributors-cli": "^6.19.0",
    "eslint": "^7.0.0",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1"
