{
  "name": "boardgamebot",
  "version": "0.9.9",
  "description": "A Discord bot to fetch board game data and a list of places you can play them online.",
  "main": "dist/src/bot.js",
  "scripts": {
    "start": "node --harmony dist/src/BoardGameBot.js",
    "build": "tsc -p .",
    "build-clear": "rm -rf dist && tsc -p .",
    "dev": "tsc -p . && node --harmony dist/src/BoardGameBot.js",
    "dev-clear": "rm -rf dist && tsc -p . && node --harmony dist/src/BoardGameBot.js",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "lint-fix": "eslint src --ext .ts --fix",
    "contrib:add": "all-contributors add"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tawilkinson/boardgamebot.git"
  },
  "author": "T A Wilkinson",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/tawilkinson/boardgamebot/issues"
  },
  "homepage": "https://github.com/tawilkinson/boardgamebot#readme",
  "dependencies": {
	"@discordjs/collection": "^0.2.1",
	"@sapphire/decorators": "next",
	"@sapphire/discord-utilities": "^2.1.5",
	"@sapphire/discord.js-utilities": "next",
	"@sapphire/fetch": "next",
	"@sapphire/framework": "next",
	"@sapphire/plugin-api": "next",
	"@sapphire/plugin-editable-commands": "^0.1.0-next.9af3e85.0",
	"@sapphire/plugin-logger": "next",
	"@sapphire/plugin-subcommands": "next",
	"@sapphire/time-utilities": "next",
	"@sapphire/type": "^2.1.0",
	"@sapphire/utilities": "^2.0.1",
	"@skyra/editable-commands": "^2.1.0",
	"discord.js": "^13.1.0",
	"dotenv-cra": "^3.0.1",
	"reflect-metadata": "^0.1.13",
	"module-alias": "^2.2.2"
