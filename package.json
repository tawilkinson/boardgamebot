		"@sapphire/type": "^2.1.0",
		"@sapphire/utilities": "^2.0.1",
		"@skyra/editable-commands": "^2.1.0",
		"discord.js": "^13.1.0",
		"dotenv-cra": "^3.0.1",
		"module-alias": "^2.2.2",
		"reflect-metadata": "^0.1.13"
	},
	"_moduleAliases": {
		"@root": "dist",
		"@lib": "dist/src/lib",
		"@utils": "dist/src/lib/util"
